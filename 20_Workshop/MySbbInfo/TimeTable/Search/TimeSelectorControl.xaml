<UserControl x:Class="MySbbInfo.TimeTable.Search.TimeSelectorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:xctk="clr-namespace:Xceed.Wpf.Toolkit;assembly=WPFToolkit.Extended"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <DatePicker x:Name="DatePicker" SelectedDateChanged="DatePickerDateChanged" />
        <xctk:TimePicker x:Name="TimePicker" ValueChanged="TimePickerTimeChanged" />
    </StackPanel>
</UserControl>
