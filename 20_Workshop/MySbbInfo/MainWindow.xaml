<Window x:Class="MySbbInfo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:MySbbInfo"
        Title="My SBB Info"
        Width="570"
        Height="600"
        Icon="img/train.ico"
        Loaded="Window_Loaded_1">
    <DockPanel>
        <TabControl DockPanel.Dock="Top">
            <TabItem Header="Time Table">
                <local:TimeTableControl x:Name="TimeTable" />
            </TabItem>
            <TabItem Header="Station">
                <local:StationControl x:Name="Station" />
            </TabItem>
            <TabItem Header="Search station">
                <local:SearchStation x:Name="SearchStation" />
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
