<Window x:Class="MySbbInfo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:MySbbInfo"
        Title="MainWindow"
        Width="525"
        Height="350" Loaded="Window_Loaded_1">
    <TabControl>
        <TabItem Header="Fahrplan">
            <local:TimeTableControl x:Name="TimeTable" />
        </TabItem>
        <TabItem Header="Station">
            <local:StationControl x:Name="Station" />
        </TabItem>
        <TabItem Header="Station suchen">
            <local:SearchStation x:Name="SearchStation" />
        </TabItem>
    </TabControl>
</Window>
